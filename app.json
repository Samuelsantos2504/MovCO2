{
  "expo": {
    "name": "MovCO2",
    "slug": "MovCO2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.samuel.movco2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Necesitamos acceso a tu ubicaci√≥n para mostrar las rutas"
        }
      ]
    ],
    "permissions": [
      "LOCATION"
    ],
    "extra": {
      "eas": {
        "projectId": "730eb684-5e17-4229-a1d8-bf5f6efd20eb"
      }
    },
    "owner": "samuel102504"
  }
}
